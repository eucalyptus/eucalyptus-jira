3Dmedium%26ownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue"},"displayName":"Sang-Min Park","active":false,"timeZone":"America/Los_Angeles"},"created":"2013-06-14T15:42:20.943-0500","updated":"2013-06-14T15:42:20.943-0500"},{"self":"https://eucalyptus.atlassian.net/rest/api/2/issue/21879/comment/47560","id":"47560","author":{"self":"https://eucalyptus.atlassian.net/rest/api/2/user?username=shashi","name":"shashi","key":"shashi","accountId":"557058:cc7ee5dc-1d4f-46c5-8068-6ae350275ba6","avatarUrls":{"48x48":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=48&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3FownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","24x24":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=24&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dsmall%26ownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","16x16":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=16&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dxsmall%26ownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","32x32":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=32&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dmedium%26ownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue"},"displayName":"Shashi Mysore","active":true,"timeZone":"America/Los_Angeles"},"body":"[~spark], I think I know the answer to this, but I'm going to ask you and other SMEs just to be sure. Is there any impact of not having xenpv on our AWS <-> Eucalyptus image migration story? ([~gholms], [~hspencer], [~dshekhawat], [~dmitrii], ... ?). \n\nAlso, us not having having xenpv drivers by default in the image does not preclude our users from installing them at a later point, if they need to (for various other reasons - Eucalyptus community efforts on Xen, etc.). Is that correct? ","updateAuthor":{"self":"https://eucalyptus.atlassian.net/rest/api/2/user?username=shashi","name":"shashi","key":"shashi","accountId":"557058:cc7ee5dc-1d4f-46c5-8068-6ae350275ba6","avatarUrls":{"48x48":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=48&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3FownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","24x24":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=24&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dsmall%26ownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","16x16":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=16&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dxsmall%26ownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","32x32":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=32&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dmedium%26ownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue"},"displayName":"Shashi Mysore","active":true,"timeZone":"America/Los_Angeles"},"created":"2013-06-14T15:49:27.444-0500","updated":"2013-06-14T15:49:27.444-0500"},{"self":"https://eucalyptus.atlassian.net/rest/api/2/issue/21879/comment/47562","id":"47562","author":{"self":"https://eucalyptus.atlassian.net/rest/api/2/user?username=spark","name":"spark","key":"spark","accountId":"557058:2ed923b8-7172-41f0-a425-8796bd9a62f9","avatarUrls":{"48x48":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=48&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3FownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue","24x24":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=24&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dsmall%26ownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue","16x16":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=16&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dxsmall%26ownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue","32x32":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=32&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dmedium%26ownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue"},"displayName":"Sang-Min Park","active":false,"timeZone":"America/Los_Angeles"},"body":"Correct that we don't preclude installing xenpv later. Regarding AWS<-> euca image story, I think the only viable option is to use EC2 Import/Export api as the image type 'raw'. It appears that during the import, ec2 does not require installing additional drivers on different hypervisor types (vmware, hyperv). Probably they emulate at the hypervisor level the default driver that's in all windows Versions (that's my theory without testing).","updateAuthor":{"self":"https://eucalyptus.atlassian.net/rest/api/2/user?username=spark","name":"spark","key":"spark","accountId":"557058:2ed923b8-7172-41f0-a425-8796bd9a62f9","avatarUrls":{"48x48":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=48&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3FownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue","24x24":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=24&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dsmall%26ownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue","16x16":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=16&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dxsmall%26ownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue","32x32":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=32&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dmedium%26ownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue"},"displayName":"Sang-Min Park","active":false,"timeZone":"America/Los_Angeles"},"created":"2013-06-14T16:04:52.838-0500","updated":"2013-06-14T16:04:52.838-0500"},{"self":"https://eucalyptus.atlassian.net/rest/api/2/issue/21879/comment/47588","id":"47588","author":{"self":"https://eucalyptus.atlassian.net/rest/api/2/user?username=shashi","name":"shashi","key":"shashi","accountId":"557058:cc7ee5dc-1d4f-46c5-8068-6ae350275ba6","avatarUrls":{"48x48":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=48&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3FownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","24x24":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=24&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dsmall%26ownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","16x16":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=16&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dxsmall%26ownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","32x32":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=32&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dmedium%26ownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue"},"displayName":"Shashi Mysore","active":true,"timeZone":"America/Los_Angeles"},"body":"[~spark], If removing XenPV has no impact on AWS <-> Eucalyptus image migration, then I'd say let's deprecate it. But, can you please confirm that it has no impact? \n\nOn a related thread, including VMware tools for Windows images was brought up. I've created a ticket EUCA-6540 to track this.","updateAuthor":{"self":"https://eucalyptus.atlassian.net/rest/api/2/user?username=shashi","name":"shashi","key":"shashi","accountId":"557058:cc7ee5dc-1d4f-46c5-8068-6ae350275ba6","avatarUrls":{"48x48":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=48&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3FownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","24x24":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=24&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dsmall%26ownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","16x16":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=16&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dxsmall%26ownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","32x32":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=32&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dmedium%26ownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue"},"displayName":"Shashi Mysore","active":true,"timeZone":"America/Los_Angeles"},"created":"2013-06-14T19:50:48.080-0500","updated":"2013-06-14T19:50:48.080-0500"},{"self":"https://eucalyptus.atlassian.net/rest/api/2/issue/21879/comment/47590","id":"47590","author":{"self":"https://eucalyptus.atlassian.net/rest/api/2/user?username=spark","name":"spark","key":"spark","accountId":"557058:2ed923b8-7172-41f0-a425-8796bd9a62f9","avatarUrls":{"48x48":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=48&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3FownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue","24x24":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=24&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dsmall%26ownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue","16x16":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=16&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dxsmall%26ownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue","32x32":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=32&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dmedium%26ownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue"},"displayName":"Sang-Min Park","active":false,"timeZone":"America/Los_Angeles"},"body":"[~shashi] it sounds strange to me that xenpv is considered in the context of aws - euca image story. we neither used xenpv for that purpose nor tested it for that. Plus, xenpv cannot be installed on KVM. Since we deprecated xen, there is no reason that xenpv should be in the windows package. In other words, packaging xenpv with Euca 3.3 is a \"bug\". So the right sequence to me is 1) fix the bug, 2) investigate aws - euca image story, 3) address it as documentation task.\r\n","updateAuthor":{"self":"https://eucalyptus.atlassian.net/rest/api/2/user?username=spark","name":"spark","key":"spark","accountId":"557058:2ed923b8-7172-41f0-a425-8796bd9a62f9","avatarUrls":{"48x48":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=48&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3FownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue","24x24":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=24&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dsmall%26ownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue","16x16":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=16&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dxsmall%26ownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue","32x32":"https://avatar-cdn.atlassian.com/fec440044f6e652174838d59d9233f29?s=32&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dmedium%26ownerId%3Dspark%26avatarId%3D10900%26noRedirect%3Dtrue"},"displayName":"Sang-Min Park","active":false,"timeZone":"America/Los_Angeles"},"created":"2013-06-14T20:41:39.606-0500","updated":"2013-06-14T20:41:55.496-0500"},{"self":"https://eucalyptus.atlassian.net/rest/api/2/issue/21879/comment/47600","id":"47600","author":{"self":"https://eucalyptus.atlassian.net/rest/api/2/user?username=shashi","name":"shashi","key":"shashi","accountId":"557058:cc7ee5dc-1d4f-46c5-8068-6ae350275ba6","avatarUrls":{"48x48":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=48&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3FownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","24x24":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=24&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dsmall%26ownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","16x16":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=16&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dxsmall%26ownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","32x32":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=32&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dmedium%26ownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue"},"displayName":"Shashi Mysore","active":true,"timeZone":"America/Los_Angeles"},"body":"[~spark], Thanks for the detailed explanation. Is there a possibility that for \"2) investigate aws - euca image story\", we will need to bring back XenPV? [~graziano] and [~gregdek] seemed to suggest that this may be the case. Hence this discussion. \n\nOn the other hand, if retaining XenPV actually breaks something in KVM / VMware today, then of course, it should be marked as a bug, and fixed ASAP. Your initial description only suggested that deprecating XenPV could reduce the number of steps (ease of use vs. bug). If this is a bug, then it requires no more discussion. Let's just go ahead and fix it.","updateAuthor":{"self":"https://eucalyptus.atlassian.net/rest/api/2/user?username=shashi","name":"shashi","key":"shashi","accountId":"557058:cc7ee5dc-1d4f-46c5-8068-6ae350275ba6","avatarUrls":{"48x48":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=48&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3FownerId%3Dshashi%26avatarId%3D11700%26noRedirect%3Dtrue","24x24":"https://avatar-cdn.atlassian.com/a22dc501b633d766a04aac00a6f07d91?s=24&d=https%3A%2F%2Feucalyptus.atlassian.net%