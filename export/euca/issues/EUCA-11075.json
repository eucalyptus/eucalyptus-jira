an.net%2Fsecure%2Fuseravatar%3Fsize%3Dxsmall%26ownerId%3Dsjones%26avatarId%3D18300%26noRedirect%3Dtrue","32x32":"https://avatar-cdn.atlassian.com/037fddf6d3bf6d691a448e01136f233a?s=32&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dmedium%26ownerId%3Dsjones%26avatarId%3D18300%26noRedirect%3Dtrue"},"displayName":"Steve Jones","active":true,"timeZone":"America/Los_Angeles"},"body":"[~spark], to test you can create a template with a wait condition and manually signal it after whatever time you want to test.\r\n\r\n{code}\r\n{\r\n    \"AWSTemplateFormatVersion\" : \"2010-09-09\",\r\n\r\n    \"Description\" : \"Wait condition for manual use.\",\r\n\r\n    \"Resources\" : {\r\n\r\n        \"myWaitHandle\" : {\r\n            \"Type\" : \"AWS::CloudFormation::WaitConditionHandle\",\r\n            \"Properties\" : {\r\n            }\r\n        },\r\n\r\n        \"myWaitCondition\" : {\r\n            \"Type\" : \"AWS::CloudFormation::WaitCondition\",\r\n            \"Properties\" : {\r\n                \"Handle\" : { \"Ref\" : \"myWaitHandle\" },\r\n                 \"Timeout\" : \"6000\"\r\n            }\r\n        }\r\n    },\r\n    \"Outputs\" : {\r\n        \"ManualSignalCommand\" : {\r\n            \"Value\" : {\r\n                \"Fn::Join\" : [ \" \", [\"curl -X PUT -H 'Content-Type:' --data-binary '{\\\"Status\\\" : \\\"SUCCESS\\\",\\\"Reason\\\" : \\\"Configuration Complete\\\",\\\"UniqueId\\\" : \\\"ID1234\\\",\\\"Data\\\" : \\\"Application has completed configuration.\\\"}'\", { \"Ref\" : \"myWaitHandle\" } ] ]\r\n            }\r\n        }\r\n    }\r\n}\r\n{code}\r\n\r\n{code}\r\n# cat signal.json \r\n{\r\n  \"Status\" : \"SUCCESS\",\r\n  \"Reason\" : \"Configuration Complete\",\r\n  \"UniqueId\" : \"ID1234\",\r\n  \"Data\" : \"TEST SUCCEED.\"\r\n}\r\n\r\ncurl -T signal.json 'http://10.111.5.60:8773/services/objectstorage/cf-waitcondition-nnpq9bevkak5j/arn%3Aaws%3Acloudformation%3A%3A000241119767%3Astack/wait-1/3d3dc2ed-5760-4598-95de-c51907630965/myWaitHandle/WaitHandle?Expires=1436261329&AWSAccessKeyId=AKIACZXC6KO4S3MH6HDA&x-amz-security-token=ZXVjYQABz5WzG%2BMsR36vRb3kB5fYmekE89zm6EqofJ9Z7wWavlnKtlQwx0q16y%2BXkdG7fzp%2B0CbpM0p%2F9%2Bh71kNxTAqtibTIACxVOqSfe9W1KLAynBL0PsL63KLhjCmXCHaCWl4jmDkuvxNHjapX1TMzl%2FK%2FKrY3Ys0zfU0gV1PqsDVzIM6R5RZ%2FlOAxGUCzp%2BgevVta%2FQpiD56THuope2n57CTp1J6ttRw%2BHcsnwHxaTIUjR186F7y5bFCh&Signature=e08c5fqT%2FSj%2B54dGXaYRRts%2B6bM%3D'\r\n{code}\r\n\r\nIIRC you will not see the output until the stack is complete so need to check the stack resources (or events?) to find the right URL to signal.","updateAuthor":{"self":"https://eucalyptus.atlassian.net/rest/api/2/user?username=sjones","name":"sjones","key":"sjones","accountId":"557058:f5dce1b0-db45-4dd6-a0e8-3cea5effb5ce","avatarUrls":{"48x48":"https://avatar-cdn.atlassian.com/037fddf6d3bf6d691a448e01136f233a?s=48&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3FownerId%3Dsjones%26avatarId%3D18300%26noRedirect%3Dtrue","24x24":"https://avatar-cdn.atlassian.com/037fddf6d3bf6d691a448e01136f233a?s=24&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dsmall%26ownerId%3Dsjones%26avatarId%3D18300%26noRedirect%3Dtrue","16x16":"https://avatar-cdn.atlassian.com/037fddf6d3bf6d691a448e01136f233a?s=16&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dxsmall%26ownerId%3Dsjones%26avatarId%3D18300%26noRedirect%3Dtrue","32x32":"https://avatar-cdn.atlassian.com/037fddf6d3bf6d691a448e01136f233a?s=32&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dmedium%26ownerId%3Dsjones%26avatarId%3D18300%26noRedirect%3Dtrue"},"displayName":"Steve Jones","active":true,"timeZone":"America/Los_Angeles"},"created":"2015-10-08T17:11:07.922-0500","updated":"2015-10-08T17:11:07.922-0500"},{"self":"https://eucalyptus.atlassian.net/rest/api/2/issue/37110/comment/93546","id":"93546","author":{"self":"https://eucalyptus.atlassian.net/rest/api/2/user?username=spark","name":"spark","key":"