ARE_URL=http://euare.euca-qa.com:8773/services/Euare\r\nexport TOKEN_URL=http://tokens.euca-qa.com:8773/services/Tokens\r\nexport AWS_AUTO_SCALING_URL=http://autoscaling.euca-qa.com:8773/services/AutoScaling\r\nexport AWS_CLOUDWATCH_URL=http://cloudwatch.euca-qa.com:8773/services/CloudWatch\r\nexport AWS_ELB_URL=http://loadbalancing.euca-qa.com:8773/services/LoadBalancing\r\nexport EUSTORE_URL=http://emis.eucalyptus.com/\r\nexport EC2_PRIVATE_KEY=${EUCA_KEY_DIR}/euca2-admin-855f73d9-pk.pem\r\nexport EC2_CERT=${EUCA_KEY_DIR}/euca2-admin-855f73d9-cert.pem\r\nexport EC2_JVM_ARGS=-Djavax.net.ssl.trustStore=${EUCA_KEY_DIR}/jssecacerts\r\nexport EUCALYPTUS_CERT=${EUCA_KEY_DIR}/cloud-cert.pem\r\nexport EC2_ACCOUNT_NUMBER='498025213678'\r\nexport EC2_ACCESS_KEY='X6NYVAAM2DACVF1KXCWP2'\r\nexport EC2_SECRET_KEY='whAwuqR8zCS3gRLT9bv04xHdav8QMdkH6rv2kaRU'\r\nexport AWS_ACCESS_KEY='X6NYVAAM2DACVF1KXCWP2'\r\nexport AWS_SECRET_KEY='whAwuqR8zCS3gRLT9bv04xHdav8QMdkH6rv2kaRU'\r\nexport AWS_CREDENTIAL_FILE=${EUCA_KEY_DIR}/iamrc\r\nexport EC2_USER_ID='498025213678'\r\nalias ec2-bundle-image=\"ec2-bundle-image --cert ${EC2_CERT} --privatekey ${EC2_PRIVATE_KEY} --user ${EC2_ACCOUNT_NUMBER} --ec2cert ${EUCALYPTUS_CERT}\"\r\nalias ec2-upload-bundle=\"ec2-upload-bundle -a ${EC2_ACCESS_KEY} -s ${EC2_SECRET_KEY} --url ${S3_URL}\"\r\n[root@clcfrontend admin]#\r\n{code}","customfield_13200":null,"customfield_11300":null,"customfield_11500":{"self":"https://eucalyptus.atlassian.net/rest/api/2/user?username=neil","name":"neil","key":"neil","accountId":"557058:bdcb5450-c2ec-4e86-a815-f842a934f480","avatarUrls":{"48x48":"https://avatar-cdn.atlassian.com/7497770f3e2aaa247f356838f1fd8ba2?s=48&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3FownerId%3Dneil%26avatarId%3D10703%26noRedirect%3Dtrue","24x24":"https://avatar-cdn.atlassian.com/7497770f3e2aaa247f356838f1fd8ba2?s=24&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3Fsize%3Dsmall%26ownerId%3Dneil%26avatarId%3D10703