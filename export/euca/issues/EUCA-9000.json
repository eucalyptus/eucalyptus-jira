sian.net%2Fsecure%2Fuseravatar%3Fsize%3Dmedium%26ownerId%3Dtony%26avatarId%3D16600%26noRedirect%3Dtrue"},"displayName":"Tony Beckham","active":true,"timeZone":"America/Tijuana"},"body":"[~spark], yeah, don't think it's cert contents related, the ssl_termination test that used to pass fails with the same error. The cert and key used there was not generated the same way (is not x509)\r\n\r\n{code}\r\n    def create_ssl_cert(self):\r\n        \"\"\"Create a SSL certificate and key\"\"\"\r\n        cert_dir = \"./test_data\"\r\n        cert_file = \"ssl_server_certs_basics.crt\"\r\n        key_file = \"ssl_server_certs_basics.pem\"\r\n\r\n        # create a key pair\r\n        k = crypto.PKey()\r\n        k.generate_key(crypto.TYPE_RSA, 1024)\r\n\r\n        # create a self-signed cert\r\n        cert = crypto.X509()\r\n        cert.get_subject().C = \"US\"\r\n        cert.get_subject().ST = \"California\"\r\n        cert.get_subject().L = \"Granite Bay\"\r\n        cert.get_subject().O = \"Eucalyptus\"\r\n        cert.get_subject().OU = \"Quality\"\r\n        cert.get_subject().CN = gethostname()\r\n        cert.set_serial_number(1000)\r\n        cert.gmtime_adj_notBefore(0)\r\n        cert.gmtime_adj_notAfter(10 * 365 * 24 * 60 * 60)\r\n        cert.set_issuer(cert.get_subject())\r\n        cert.set_pubkey(k)\r\n        cert.sign(k, 'sha1')\r\n\r\n        open(join(cert_dir, cert_file), \"wt\").write(\r\n            crypto.dump_certificate(crypto.FILETYPE_PEM, cert))\r\n        open(join(cert_dir, key_file), \"wt\").write(\r\n            crypto.dump_privatekey(crypto.FILETYPE_PEM, k))\r\n{code}","updateAuthor":{"self":"https://eucalyptus.atlassian.net/rest/api/2/user?username=tony","name":"tony","key":"tony","accountId":"557058:353956db-b78c-4f3b-9096-f8f58e6709c8","avatarUrls":{"48x48":"https://avatar-cdn.atlassian.com/14ec1b18e630cc0743492fc327a12df7?s=48&d=https%3A%2F%2Feucalyptus.atlassian.net%2Fsecure%2Fuseravatar%3FownerId%3Dtony%26avatarId%3D16600%26noRedirect%3