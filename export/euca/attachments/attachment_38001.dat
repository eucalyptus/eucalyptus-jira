# cat snaps.sh
#!/bin/bash
set -x
POOL=$1
VOLUME=$2
SNAPSTART=$3
SNAPEND=$4
PARALLEL=$5
if [ -n "$PARALLEL" ]; then
  echo Will run commands in parallel
else
  echo Will run commands serially
fi
PREVSNAP=$(($SNAPSTART-1))
for SNAP in $(seq $(($SNAPSTART)) $SNAPEND); do
  COMMAND="snap.sh $POOL $VOLUME manual-snap-$SNAP manual-clone-$SNAP manual-snap-$PREVSNAP"
  if [ -n "$PARALLEL" ]; then
    $COMMAND &
  else
    $COMMAND
  fi
  PREVSNAP=$SNAP
done

