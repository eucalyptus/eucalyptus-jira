# cat snap.sh
#!/bin/bash
set -x
RBDCMD="rbd --id qauser --keyring /etc/ceph/ceph.client.qauser.keyring"
POOL=$1
VOLUME=$2
SNAP=$3
CLONE=$4
PREVSNAP=$5
DIFFPATH=/var/tmp/snap-$VOLUME-$PREVSNAP-$SNAP.diff
$RBDCMD snap create $POOL/$VOLUME@$SNAP
echo $? returned from snap create $POOL/$VOLUME@$SNAP
$RBDCMD snap protect $POOL/$VOLUME@$SNAP
echo $? returned from snap protect $POOL/$VOLUME@$SNAP
$RBDCMD clone $POOL/$VOLUME@$SNAP $POOL/$CLONE
echo $? returned from clone $POOL/$VOLUME@$SNAP
$RBDCMD export-diff $POOL/$VOLUME@$SNAP $DIFFPATH --from-snap $PREVSNAP
echo $? returned from export-diff $POOL/$VOLUME@$SNAP
/bin/rm $DIFFPATH

